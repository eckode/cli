    "@types/node": "^22.10.1",
    "@types/resolve-bin": "^0.4.3",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "chalk": "^5.3.0",
    "eslint": "^9.16.0",
    "eslint-config-love": "^110.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.0",
    "eslint-plugin-promise": "^7.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2"
  },
  "main": "./src/index.mjs",
  "dependencies": {}

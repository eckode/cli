{
  "name": "@eckode/cli",
  "version": "0.0.5",
  "description": "CLI tool, written in TypeScript, for running @eckode scoped packages.",
  "keywords": [
    "eckode",
    "webpack",
    "typescript",
    "JavaScript",
    "CLI"
  ],
  "author": "Jared Rethman <jaredrethman@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eckode/cli.git"
  },
  "bugs": {
    "url": "https://github.com/eckode/cli/issues"
  },
  "homepage": "https://github.com/eckode/cli",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "ts-node-esm ./src/clean.mts",
    "pkg": "tsc && ts-node-esm ./src/package.mts"
  },
  "bin": {
    "eckode": "./bin/eckode.mjs"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "files": [
    "bin",
    "src",
    "package.json",
    ".eslintrc.js",
    "prettier.js",
    "tsconfig.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.13",
    "@types/resolve-bin": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "chalk": "^5.2.0",
    "eslint": "^8.39.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "resolve-bin": "^1.0.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "main": "./src/index.mjs",
  "dependencies": {
    "rimraf": "^5.0.0"
  }
}
